<Page
	x:Class="SudokuStudio.Views.Pages.ContentDialogs.ExceptionThrownOnAnalyzingContent"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:pd="using:SudokuStudio.Views.Pages.ContentDialogs"
	xmlns:m="using:SudokuStudio.Markup"
	mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=pd:ExceptionThrownOnAnalyzingContent}">
	<StackPanel Orientation="Vertical" Spacing="10">
		<TextBlock Text="{m:R Key=AnalyzePage_ExceptionThrown}" TextWrapping="WrapWholeWords" />
		<TextBlock x:Name="ExceptionDisplayer" TextWrapping="WrapWholeWords" IsTextSelectionEnabled="True" />
	</StackPanel>
</Page>
