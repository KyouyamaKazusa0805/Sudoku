<Page
	x:Class="SudokuStudio.Views.Pages.TechniqueDebuggerPage"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:c="using:SudokuStudio.Views.Controls"
	xmlns:p="using:SudokuStudio.Views.Pages"
	mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=p:TechniqueDebuggerPage}">
	<StackPanel Margin="{StaticResource ContentAlignedWithNavigationViewHeaderMargin}" Spacing="6">
		<c:TechniqueView
			HorizontalAlignment="Left" SelectionMode="None" HorizontalSpacing="6" VerticalSpacing="6"
			MaxWidth="800" MaxHeight="500" />
	</StackPanel>
</Page>
