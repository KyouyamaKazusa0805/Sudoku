<UserControl
	x:Class="SudokuStudio.Views.Controls.Shapes.CircleRing"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:s="using:SudokuStudio.Views.Controls.Shapes"
	mc:Ignorable="d" d:DataContext="{d:DesignInstance Type=s:CircleRing}">
	<Ellipse x:Name="BackingEllipse" Stroke="{x:Bind Background,Mode=OneWay}" StrokeThickness="{x:Bind StrokeThickness,Mode=OneWay}" />
</UserControl>
