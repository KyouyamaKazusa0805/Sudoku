<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<RootNamespace>Sudoku</RootNamespace>
		<Description>Provides APIs that presents strategies on handling different cases around sudoku.</Description>
		<DocumentationFile>..\docxml\Sudoku.Strategying.xml</DocumentationFile>
	</PropertyGroup>

	<ItemGroup>
		<ProjectReference Include="..\Sudoku.Analytics\Sudoku.Analytics.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Resources\StrategyingResources.Designer.cs">
			<DesignTime>True</DesignTime>
			<AutoGen>True</AutoGen>
			<DependentUpon>StrategyingResources.resx</DependentUpon>
		</Compile>
	</ItemGroup>

	<ItemGroup>
		<EmbeddedResource Update="Resources\StrategyingResources.resx">
			<Generator>ResXFileCodeGenerator</Generator>
			<LastGenOutput>StrategyingResources.Designer.cs</LastGenOutput>
		</EmbeddedResource>
	</ItemGroup>

</Project>
